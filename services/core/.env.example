# Configuración del Core Service

# Configuración del servidor (se usa la configuración del .env principal)
# Los valores del .env principal tienen prioridad:
# CORE_SERVICE_HOST=0.0.0.0
# CORE_SERVICE_PORT=8001
# Valores locales como fallback:
CORE_HOST=0.0.0.0
CORE_PORT=8001

# Configuración de almacenamiento
STORAGE_PATH=./app/storage

# Configuración de logging
LOG_LEVEL=INFO

# Configuración de desarrollo
DEBUG=false
RELOAD=true
DEVELOPMENT_MODE=true
DEBUG_LEVEL=INFO

# Configuración de Supabase
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# Configuración de Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=
# Tiempo de expiración por defecto en segundos
REDIS_DEFAULT_EXPIRE=3600

# Configuración de CORS
CORS_ORIGINS=["http://localhost:3000", "http://localhost:8080"]