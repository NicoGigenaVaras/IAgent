import asyncio
from app.infrastructure.persistence.supabase.client import SupabaseClient

async def verify_interview_status():
    """Verify that the interview status was updated to 'evaluated'"""
    client = SupabaseClient()
    
    # Get the latest interview
    interviews = await client.get("interviews", {})
    
    print("📋 Interview Status Verification:")
    for interview in interviews:
        interview_id = interview.get('interview_id')
        status = interview.get('status')
        updated_at = interview.get('updated_at')
        
        print(f"   📝 Interview: {interview_id}")
        print(f"   📊 Status: {status}")
        print(f"   🕒 Updated: {updated_at}")
        print()

if __name__ == "__main__":
    asyncio.run(verify_interview_status())